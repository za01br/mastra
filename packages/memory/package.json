{
  "name": "@mastra/memory",
  "version": "0.1.0-alpha.82",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./kv-upstash": {
      "import": {
        "types": "./dist/kv/upstash.d.ts",
        "default": "./dist/kv/upstash.js"
      }
    },
    "./postgres": {
      "import": {
        "types": "./dist/postgres/index.d.ts",
        "default": "./dist/postgres/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "build": "pnpm run check && tsup src/index.ts --format esm --dts --clean --treeshake",
    "build:watch": "pnpm build --watch",
    "test:integration": "cd integration-tests && pnpm run test",
    "test": "pnpm test:integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mastra/core": "workspace:^",
    "@upstash/redis": "^1.34.3",
    "ai": "^4.0.12",
    "pg": "^8.13.1",
    "pg-pool": "^3.7.0",
    "postgres": "^3.4.5",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "tsup": "^8.0.1",
    "vitest": "^3.0.4"
