  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./kv-upstash": {
      "import": {
        "types": "./dist/kv/upstash.d.ts",
        "default": "./dist/kv/upstash.js"
      }
    },
    "./postgres": {
      "import": {
        "types": "./dist/postgres/index.d.ts",
        "default": "./dist/postgres/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "check": "tsc --noEmit",
