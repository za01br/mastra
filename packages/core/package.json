{
  "name": "@mastra/core",
  "version": "0.1.27-alpha.7",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "analyze": "size-limit --why",
    "build": "dts build",
    "build:dev": "dts watch",
    "lint": "dts lint",
    "size": "size-limit",
    "start": "dts watch",
    "test": "jest",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^1.0.3",
    "@ai-sdk/anthropic": "^0.0.51",
    "@ai-sdk/azure": "^1.0.5",
    "@ai-sdk/cohere": "^1.0.2",
    "@ai-sdk/google": "^1.0.1",
    "@ai-sdk/mistral": "^1.0.2",
    "@ai-sdk/xai": "^1.0.2",
    "@badgateway/oauth2-client": "^2.4.0",
    "@date-fns/utc": "^1.2.0",
    "@pinecone-database/pinecone": "^3.0.3",
    "@prisma/client": "^5.18.0",
    "@upstash/redis": "^1.34.3",
    "ai": "^4.0.3",
    "lodash": "^4.17.21",
    "sift": "^17.1.3",
    "xstate": "^5.19.0"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=20"
  },
  "size-limit": [
    {
      "path": "dist/mylib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "dts-cli": "^2.0.5",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "size-limit": "^11.1.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
