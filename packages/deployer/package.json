    "@types/babel__helper-module-imports": "^7.18.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.1",
    "concurrently": "^9.1.0",
    "hono-openapi": "^0.4.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "superjson": "^2.2.2",
    "tsup": "^8.3.6",
    "type-fest": "^4.30.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "zod-to-json-schema": "^3.24.1",
    "@internal/lint": "workspace:*",
    "eslint": "^9.20.1"
