{
  "name": "@mastra/deployer",
  "version": "0.1.5-alpha.3",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./build": {
      "types": "./dist/build/index.d.ts",
      "default": "./dist/build/index.js"
    },
    "./bundler": {
      "types": "./dist/bundler/index.d.ts",
      "default": "./dist/bundler/index.js"
    },
    "./analyze": {
      "types": "./dist/build/analyze.d.ts",
      "default": "./dist/build/analyze.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src/index.ts src/build/index.ts src/server/index.ts src/build/bundler.ts src/build/analyze.ts src/bundler/index.ts --format esm --clean --experimental-dts --treeshake --publicDir",
    "build:watch": "pnpm build --watch",
    "pull:openapispec": "node src/server/openapi.script.js",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.26.7",
    "@babel/helper-module-imports": "^7.25.9",
    "@mastra/core": "workspace:^",
    "@neon-rs/load": "^0.1.82",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-virtual": "^3.0.2",
    "builtins": "^5.1.0",
    "detect-libc": "^2.0.3",
    "dotenv": "^16.4.7",
    "esbuild": "^0.24.2",
    "fs-extra": "^11.2.0",
    "hono": "^4.6.17",
    "rollup": "^4.30.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/swagger-ui": "^0.5.0",
    "@microsoft/api-extractor": "^7.49.2",
