{
  "name": "mastra",
  "version": "0.1.57-alpha.29",
  "license": "MIT",
  "description": "cli for mastra",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mastra": "./dist/index.js"
  },
  "files": [
    "dist",
    "src/starter-files",
    "src/scripts",
    "agent-chat/dist"
  ],
  "scripts": {
    "build": "npx tsc && cp -r src/starter-files dist/",
    "build:dev": "npx tsc --watch",
    "build:agent-chat": "cd agent-chat && vite build",
    "dev:agent-chat": "cd agent-chat && vite",
    "init": "npx tsx src/index.ts init",
    "dev": "npx tsx src/index.ts dev",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "typecheck": "tsc --noEmit --incremental",
    "postinstall": "node src/scripts/postinstall.js"
  },
  "keywords": [
    "mastra",
    "cli",
    "ai"
  ],
  "author": "",
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "@mastra/core": "workspace:*",
    "@swc/core": "^1.9.3",
    "@types/figlet": "^1.7.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.24.0",
    "execa": "^9.3.1",
    "express": "^4.21.1",
    "figlet": "^1.8.0",
    "fs-extra": "^11.2.0",
    "is-port-reachable": "^4.0.0",
    "memfs": "^4.14.0",
    "picocolors": "^1.1.1",
    "posthog-node": "^4.3.1",
    "prettier": "^3.3.3",
    "prompt": "^1.3.0",
    "tcp-port-used": "^1.0.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.30.0",
    "typescript": "^5.5.4",
    "yocto-spinner": "^0.1.1",
    "zod": "^3.24.0",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.13.5",
    "lucide-react": "^0.468.0",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "remeda": "^2.17.4",
    "shiki": "^1.24.2",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@types/prompt": "^1.1.8",
    "@types/react": "^18",
    "@types/tcp-port-used": "^1.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "vite": "^6.0.1"
  }
}
