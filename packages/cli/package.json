{
  "name": "mastra",
  "version": "0.1.57-alpha.85",
  "license": "MIT",
  "description": "cli for mastra",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mastra": "./dist/index.js"
  },
  "files": [
    "dist",
    "src/starter-files",
    "src/scripts",
    "src/playground/dist",
    "node_modules"
  ],
  "scripts": {
    "build": "npm-run-all build:tsc copy-starter-files build:playground",
    "build:tsc": "tsc",
    "build:dev": "npx tsc --watch",
    "copy-starter-files": "cp -r src/starter-files dist/",
    "build:playground": "cd src/playground && vite build",
    "dev:playground": "cd src/playground && vite",
    "init": "npx tsx src/index.ts init",
    "dev": "npx tsx src/index.ts dev",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "typecheck": "tsc --noEmit --incremental",
    "postinstall": "node src/scripts/postinstall.js"
  },
  "keywords": [
    "mastra",
    "cli",
    "ai"
  ],
  "author": "",
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "@dagrejs/dagre": "^1.1.4",
    "@hookform/resolvers": "^3.9.0",
